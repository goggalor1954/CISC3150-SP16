template <typename T>
class Node {
public:
T nodeValue;
Node<T> *next;
Node (const T& item, Node<T> *ptr = NULL): nodeValue(item), next(ptr) {}
};

template <typename T>
class Queue: public Node{
	private:
	Queue *head, *tail;
	int size;

	public:
	//default constructor
	Queue( T newEle){
		nodeValue=newEle;
		this->*head;
		this->*tail;
		size=1;
	}
